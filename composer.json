{
    "name": "orianenmk/deepzoom",
    "version": "1.0.0",
    "description": "Deepzoom Tile Generator for OpenSeadragon",
    "license": "MIT",
    "keywords": [
        "openseadragon",
        "deepzoom",
        "dzi",
        "zoom",
        "zoomable",
        "tiled",
        "intervention",
        "image",
        "gd",
        "imagemagick",
        "imagick",
        "flysystem"
    ],
    "autoload": {
        "psr-4": {
            "Orianenmk\\Deepzoom\\": "src/"
        }
    },
    "autoload-deb": {
      "psr-4": {
        "Orianenmk\\Deepzoom\\Tests\\": "tests/"
      }
    },
    "require": {
        "php": ">=5.4.0",
        "intervention/image": "^2.3",
        "league/flysystem": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
